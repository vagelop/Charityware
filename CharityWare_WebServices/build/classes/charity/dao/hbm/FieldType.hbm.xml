<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping>
   <class name="charity.model.FieldType" table="field_type">
      <meta attribute="class-description">
         This class contains the filled form detail. 
      </meta>
      <id name="fieldType_id" type="int" column="field_type_id">
         <generator class="native"/>
      </id>
      <property name="field_type" column="field_type" type="string"/>
      <property name="field_dataType" column="field_DataType" type="string"/>
      <property name="field_Description" column="field_Description" type="string"/>
      <property name="isActive" column="isActive" type="boolean"/>
   </class>
</hibernate-mapping>